{
  "main": "app-post-to-local-device.min.js",
  "version": "1",
  "sheetSettings": {
    "defaultCols": 1,
    "defaultRows": 1,
    "minCols": 1,
    "minRows": 1
  },
  "inputs": [
    {
      "key": "info",
      "type": "Group",
      "label": "Info",
